# RevenueCat API Keys
# Get these from your RevenueCat Dashboard: https://app.revenuecat.com
# Copy this file to .env and replace with your actual API keys

# iOS API Key (starts with appl_)
REVENUECAT_IOS_API_KEY=appl_your_ios_api_key_here

# Android API Key (starts with goog_)
REVENUECAT_ANDROID_API_KEY=goog_your_android_api_key_here

# Development settings
REVENUECAT_BYPASS_PAYWALL=false
REVENUECAT_DEBUG_LOGS=true
